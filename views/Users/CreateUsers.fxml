<BorderPane fx:id="student" prefHeight="700.0" prefWidth="1173.0" stylesheets="@../../src/com/faculty/css/fullpackstyling.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.faculty.fxcontrollers.users.UserRegistrationController">
   <top>
      <ToolBar prefHeight="50.0" prefWidth="1173.0" style="-fx-background-color: whitesmoke;" BorderPane.alignment="CENTER">
         <items>
            <JFXButton fx:id="backBtn" alignment="CENTER" buttonType="RAISED" onAction="#backBtnAction" prefHeight="30.0" prefWidth="56.0" style="-fx-background-color: #2D75E8;" text="Back" textFill="WHITE" translateX="10.0">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </JFXButton>
         </items>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </ToolBar>
   </top>
   <bottom>
      <ToolBar prefHeight="44.0" prefWidth="1163.0" style="-fx-background-color: whitesmoke;" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <items>
            <JFXButton fx:id="createBtn" buttonType="RAISED" onAction="#createUserAction" prefHeight="30.0" prefWidth="65.0" style="-fx-background-color: #2D75E8;" text="Create" textFill="WHITE" translateX="10.0">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></JFXButton>
            <JFXButton fx:id="cancelBtn" buttonType="RAISED" onAction="#cancelUserFormAction" prefHeight="30.0" prefWidth="65.0" stylesheets="@/com/faculty/css/fullpackstyling.css" text="Cancel" textFill="WHITE" translateX="20.0">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></JFXButton>
         </items>
      </ToolBar>
   </bottom>
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
         <children>
            <JFXTextField fx:id="firstNameFld" focusColor="#2d75e8" labelFloat="true" maxWidth="250.0" prefHeight="25.0" prefWidth="250.0" promptText="First Name">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="lastNameFld" focusColor="#2d75e8" labelFloat="true" layoutX="472.0" layoutY="301.0" maxWidth="250.0" prefHeight="25.0" prefWidth="250.0" promptText="Last Name">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="usernameFld" focusColor="#2d75e8" labelFloat="true" layoutX="472.0" layoutY="336.0" maxWidth="250.0" prefHeight="25.0" prefWidth="250.0" promptText="Username">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXPasswordField fx:id="passwordFld" focusColor="#2d75e8" labelFloat="true" maxWidth="250.0" promptText="Password" style="-fx-font-weight: bold;">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </JFXPasswordField>
            <JFXTextField fx:id="emailFld" focusColor="#2d75e8" labelFloat="true" layoutX="472.0" layoutY="318.0" maxWidth="250.0" prefHeight="25.0" prefWidth="250.0" promptText="Email">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="phoneFld" focusColor="#2d75e8" labelFloat="true" layoutX="472.0" layoutY="371.0" maxWidth="250.0" prefHeight="25.0" prefWidth="250.0" promptText="Phone">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </JFXTextField>
         </children>
      </VBox>
   </center>
</BorderPane>
